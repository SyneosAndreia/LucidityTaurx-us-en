<div class="list-view-partial">

	<h1>{{ summary }}</h1>

	{{#locations}}
    <div class="location-item">
		<h2>
			{{{info.title }}}
			{{#distance_from_client}}
				<span class="distance-span">({{ distance_from_client }}mi)</span>
			{{/distance_from_client}}
		</h2>
		<p>{{ info.address.street }}</p>
		<p>{{ info.address.city }}, {{ info.address.state }} {{ info.address.zip }}</p>
		<div class="google-maps-link" onclick="window.open(encodeURI('http://maps.google.com/maps?q={{ info.address.street }} {{ info.address.city }}, {{ info.address.state }} {{ info.address.zip }}'))">
			<p>OPEN IN GOOGLE MAPS</p><img src="data:image/png;base64,">

		</div>
		<p class="info-name">{{ info.contact.name }}</p>
		<p>{{ info.contact.phone }}</p>
		<a class="info-email" href="mailto:{{ info.contact.email }}">{{ info.contact.email }}</a>
    </div>
  	{{/locations }}

</div>